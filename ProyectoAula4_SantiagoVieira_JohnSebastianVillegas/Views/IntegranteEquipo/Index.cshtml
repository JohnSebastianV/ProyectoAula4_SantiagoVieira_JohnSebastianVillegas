@model IEnumerable<ProyectoAula4_SantiagoVieira_JohnSebastianVillegas.IntegranteEquipo>

@{
    ViewBag.Title = "Lista Integrantes de equipos";
}

<h2>Lista Integrantes de equipos</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Identificacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Apellido)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rol)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
